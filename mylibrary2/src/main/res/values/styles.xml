<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- start commonui-->
    <style name="ici28c_togglebutton_style">
        <item name="android:gravity">center</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="ici_dialog_style" parent="android:style/Theme.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@drawable/ici28c_dialog_transport_bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="ici_doalog_anim" parent="android:Animation">
        <!--//进入时的动画-->
        <item name="android:windowEnterAnimation">@anim/ici28c_dialog_enter</item>
        <!--//退出时的动画-->
        <item name="android:windowExitAnimation">@anim/ici28c_dialog_exit</item>
    </style>
    <!-- end commonui-->
</resources>